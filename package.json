{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "test": "umi test",
    "lint": "npm-run-all lint:ts lint:es",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx src mock tests",
    "lint:es:fix": "eslint --ext .js,.jsx,.ts,.tsx src mock --fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:ts:fix": "tslint --fix \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:prettier:fix": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:style": "stylelint 'src/**/*.less'",
    "lint:style:fix": "stylelint --fix 'src/**/*.less'",
    "tsc": "./node_modules/.bin/tsc --noEmit",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.7.0",
    "stylint": "^1.5.9",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.6.1",
    "umi-plugin-react": "^1.6.0",
    "umi-types": "^0.2.0",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-order": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run lint",
      "commit-msg": "node scripts/commit.validate.js"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
