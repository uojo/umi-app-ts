{
  "defaultSeverity": "error",
  "extends": [
    "tslint-eslint-rules",
    "tslint-react"
  ],
  "jsRules": false,
  "rules": {
    "no-any": true,
    "ban-types": [true, ["Object", "Use {} instead."],
      ["String"]
    ],
    "no-construct": true,
    "eofline": true,
    "no-debugger": true,
    "no-reference": true
  }
}
